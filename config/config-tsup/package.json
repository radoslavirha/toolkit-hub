{
    "name": "@radoslavirha/config-tsup",
    "version": "0.0.4",
    "description": "Base tsup configurations",
    "keywords": [
        "tsup"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com:radoslavirha/toolkit-hub.git"
    },
    "exports": {
        "import": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "require": {
            "types": "./dist/index.d.cts",
            "require": "./dist/index.cjs"
        }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "package.json",
        "README.md"
    ],
    "scripts": {
        "build": "tsup",
        "cleanup:modules": "rm -rf node_modules",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "prepublishOnly": "pnpm build"
    },
    "devDependencies": {
        "@radoslavirha/config-eslint": "workspace:*",
        "@radoslavirha/config-typescript": "workspace:*"
    },
    "peerDependencies": {
        "tsup": "catalog:"
    },
    "engines": {
        "node": ">= 22.14.0",
        "pnpm": ">= 9.14.4"
    },
    "os": [
        "darwin",
        "linux"
    ],
    "publishConfig": {
        "registry": "https://npm.pkg.github.com"
    },
    "README": "README.md"
}