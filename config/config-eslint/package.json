{
    "name": "@radoslavirha/config-eslint",
    "version": "0.2.1",
    "description": "Eslint configuration",
    "keywords": [
        "eslint",
        "eslintconfig",
        "eslint-config"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com:radoslavirha/toolkit-hub.git"
    },
    "type": "module",
    "main": "src/index.mjs",
    "files": [
        "src",
        "package.json",
        "README.md"
    ],
    "scripts": {
        "cleanup:modules": "rm -rf node_modules",
        "lint": "eslint .",
        "lint:fix": "eslint --fix ."
    },
    "dependencies": {
        "@stylistic/eslint-plugin": "5.6.1"
    },
    "peerDependencies": {
        "eslint": "catalog:",
        "typescript-eslint": "catalog:"
    },
    "engines": {
        "node": ">= 24",
        "pnpm": ">= 10"
    },
    "os": [
        "darwin",
        "linux"
    ],
    "publishConfig": {
        "registry": "https://npm.pkg.github.com"
    },
    "README": "README.md"
}