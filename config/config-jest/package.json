{
    "name": "@radoslavirha/config-jest",
    "version": "0.0.3",
    "description": "Jest configuration and coverage tresholds",
    "keywords": [
        "jest",
        "test"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com:radoslavirha/toolkit-hub.git"
    },
    "type": "module",
    "exports": {
        "import": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "require": {
            "types": "./dist/index.d.cts",
            "require": "./dist/index.cjs"
        }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "package.json",
        "README.md"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts --clean",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "prepublishOnly": "pnpm build"
    },
    "dependencies": {
        "jest-html-reporters": "^3.1.7",
        "jest-junit": "^16.0.0"
    },
    "devDependencies": {
        "@jest/types": "^29.6.3",
        "@radoslavirha/config-eslint": "workspace:*",
        "@radoslavirha/config-typescript": "workspace:*",
        "tsup": "catalog:"
    },
    "peerDependencies": {
        "@swc/core": "catalog:",
        "@swc/jest": "catalog:",
        "@types/jest": "catalog:",
        "jest": "catalog:"
    },
    "engines": {
        "node": ">= 20.12.1",
        "pnpm": ">= 9.14.4"
    },
    "os": [
        "darwin",
        "linux"
    ],
    "publishConfig": {
        "registry": "https://npm.pkg.github.com"
    },
    "README": "README.md"
}